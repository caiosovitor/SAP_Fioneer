{
  "compilerOptions": {
    "target": "es6",
    "module": "commonjs",
    "lib": ["ESNext", "DOM"],
    "types": ["cypress", "node"],
    "moduleResolution": "node",
    "esModuleInterop": true,
    "skipLibCheck": true,
    "strict": true,
    "forceConsistentCasingInFileNames": true,
    "resolveJsonModule": true,
    "isolatedModules": true
  },
  "include": ["cypress/**/*.ts"],
  "exclude": ["node_modules"]
}
